{
  "name": "CommsCompanion",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev-start": "nodemon server.js ",
    "watch": "watchify index.js -o client/bundle.js -dv -t babelify",
    "build": "browserify . -o client/bundle.js -g uglifyify",
    "test": "ava --watch | tap-spec"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "ava": {
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/author/CommsCompanion"
  },
  "author": "author",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/author/CommsCompanion/issues"
  },
  "homepage": "https://github.com/author/CommsCompanion",
  "dependencies": {
    "body-parser": "^1.15.0",
    "dotenv": "^2.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "express": "^4.13.4",
    "knex": "^0.10.0",
    "level": "^1.4.0",
    "path": "^0.12.7",
    "pg": "^4.5.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.2.0",
    "redtape": "^1.0.0",
    "retape": "0.0.3",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sqlite3": "^3.1.3",
    "superagent": "^1.8.3",
    "supertest": "^1.2.0",
    "tape": "^4.5.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint-plugin-react": "^5.0.1",
    "nodemon": "^1.9.1",
    "react-addons-test-utils": "^15.0.1",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
}
